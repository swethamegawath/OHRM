package com.Inheritance;

import org.apache.bcel.generic.RETURN;

public class Base 
{
	public  void Addition(c)
	{
		int a=20;
		int b=20;
		int c;
		c=a+b;
		System.out.println("addition of two numbers is : "+c);
		RETURN c;
	}
	
	public void Subtraction()
	{
		int a=20;
		int b=20;
		int c;
		c=a-b;
		System.out.println("addition of two numbers is : "+c);
		
	}
	
	public void Division() 
	{
		int a=20;
		int b=20;
		int c;
		c=a/b;
		System.out.println("addition of two numbers is : "+c);
		
	}
}
